/* pages/center/center.wxss */
.container {
  min-height: 100vh;
  background: #f5f5f5;
  display: flex;
  flex-direction: column;
}
.center-profile {
  padding: 20rpx;
  background: #ffffff;
  display: flex;
  align-items: center;
}
.center-profile .profile-authbtn{
  display: inline-block;
  margin-right: auto;
}
.profile-authbtn button {
  border-radius: 20px;
  padding: 12rpx 24rpx;
  font-size: 12rpx;
}